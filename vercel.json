{
    "builds" : [{
        "src": "GymManagementDjango/wsgi.py",
        "use": "@vercel/python",
        "config" : { "maxLambdaSize": "50mb", "runtime": "python3.9"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "GymManagementDjango/wsgi.py"
        }
    ],
    "functions": {
        "api/*.py": {
            "maxDuration": 10,
            "memory": 512
        }
    }
}
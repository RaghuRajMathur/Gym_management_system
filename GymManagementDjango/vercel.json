{
    "builds" : [{
        "src": "Gym_management_system/wsgi.py",
        "use": "@vercel/python",
        "config" : { "maxLambdaSize": "15mb", "runtime": "python3.9"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Gym_management_system/wsgi.py"
        }
    ]
}